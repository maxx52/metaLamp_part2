.find-room
  h1.find-room-title Найдём номера под ваши пожелания
  form.find-room-form
    +field-group('t-field', 'Прибытие', 'arrival', 'Выезд', 'checkOut')(placeholder="ДД.ММ.ГГГГ")
    .find-room-item.find-room-guests
      +field-dropdown('Гости', 'guests')(value="Сколько гостей")
      +btn-arrow('find-room-btn', 'Подобрать номер')